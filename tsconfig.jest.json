{
    "compilerOptions": {
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "module": "commonjs",
      "target": "es6",
      "lib": ["dom", "es6"],
      "moduleResolution": "node",
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }